{
  "jobDefinitionName": "GPUJobDefinition",
  "type": "container",
  "retryStrategy": {
    "attempts": 1
  },
  "containerProperties": {
    "vcpus": 4,
    "memory": 16000,
    "resourceRequirements": [
      {
        "type": "GPU",
        "value": "1"
      }
    ],
    "environment": [
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "us-west-2"
      }
    ],
    "image": "${ECR_REPO_URI}:latest",
    "command": ["python3", "testing.py"]
  }
}
